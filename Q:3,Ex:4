/*3.	Frequency of a Number
âž¤ Find how many times a specific number appears in a given array.
*/
#include <stdio.h>

int main()
{
    int n, i, key, count = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];

    printf("Enter %d integers:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter the number to find frequency: ");
    scanf("%d", &key);

    for(i = 0; i < n; i++) {
        if(arr[i] == key)
            count++;
    }

    printf("Frequency of %d = %d\n", key, count);

    return 0;
}
